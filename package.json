{
  "name": "weatherapp",
  "private": true,
  "version": "1.0.0",
  "description": "WeatherApp",
  "repository": "",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor/android": "^5.0.5",
    "@capacitor/app": "^5.0.3",
    "@capacitor/browser": "^5.0.3",
    "@capacitor/core": "^5.0.5",
    "@capacitor/geolocation": "^5.0.4",
    "@capacitor/ios": "^5.0.5",
    "@capacitor/keyboard": "^5.0.4",
    "@capacitor/preferences": "^5.0.4",
    "@capacitor/splash-screen": "^5.0.4",
    "@capacitor/status-bar": "^5.0.4",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "dom7": "^4.0.6",
    "framework7": "^8.1.0",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.1.0",
    "inversify": "^6.0.1",
    "material-icons": "^1.13.8",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "skeleton-elements": "^4.0.1",
    "swiper": "^9.4.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.5",
    "@types/axios": "^0.14.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-redux": "^7.1.25",
    "@vitejs/plugin-react": "^4.0.1",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^8.5.1",
    "sass": "^1.63.6",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
